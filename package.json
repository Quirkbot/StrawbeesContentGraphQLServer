{
  "name": "strawbees-content-graphql-server",
  "version": "0.0.3",
  "description": "Access to Strawbees content via graphql",
  "scripts": {
    "start": "node server.js",
    "deploy": "now --public -e SPACE_ID -e CDA_TOKEN -e CMA_TOKEN"
  },
  "now": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cf-graphql": "^0.4.3",
    "contentful": "^4.5.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6"
  },
  "devDependencies": {
    "now": "^6.2.4"
  }
}

{
  "name": "strawbees-content-graphql-server",
  "version": "0.1.3",
  "description": "Access to Strawbees content via graphql",
  "scripts": {
    "start": "node server.js",
    "deploy": "now --public -e SPACE_ID -e CDA_TOKEN -e CMA_TOKEN"
  },
  "now": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cf-graphql": "^0.5.0",
    "contentful": "^5.0.5",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11"
  },
  "devDependencies": {
    "now": "^9.1.0"
  }
}
